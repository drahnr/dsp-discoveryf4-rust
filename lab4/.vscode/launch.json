{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "openocd",
            "cwd": "${workspaceRoot}",
            /* dont forget to select whichever example you are debugging*/
            "executable": "./target/thumbv7em-none-eabihf/debug/examples/4_1_dft_calculations",
            "preLaunchTask": "rust: cargo build examples",
            "name": "Debug (OpenOCD)",
            "device": "STM32F407VGT6",
            "configFiles": [
                "board/stm32f4discovery.cfg"
            ],
            "runToMain": true,
            "gdbpath": "gdb-multiarch",
            "svdFile": "${workspaceRoot}/.vscode/STM32F407.svd"
        }
    ]
}